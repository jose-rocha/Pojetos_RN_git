      <td>Quantidade (2º)</td>
      <td>Quantidade (3º)</td>
      <td>Quantidade (4º)</td>
      <td>Quantidade (5º)</td>
      <td>Quantidade (6º)</td>
      <td>Quantidade (7º)</td>
      <td>Quantidade (8º)</td>

      {{!-- ------------------ --}}

      <td>Descrição 1</td>
      <td>Descrição 2</td>
      <td>Descrição 3</td>
      <td>Descrição 4</td>
      <td>Descrição 5</td>
      <td>Descrição 6</td>
      <td>Descrição 7</td>
      <td>Descrição 8</td>

      {{!-- ------------------ --}}

      <td>precoUni 1</td>
      <td>precoUni 2</td>
      <td>precoUni 3</td>
      <td>precoUni 4</td>
      <td>precoUni 5</td>
      <td>precoUni 6</td>
      <td>precoUni 7</td>
      <td>precoUni 8</td>

      {{!-- ------------------ --}}

      <td>frete 1</td>
