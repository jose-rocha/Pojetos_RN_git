      <td class="frete2">R$ {{frete2}},00</td>
      <td class="frete3">R$ {{frete3}},00</td>
      <td class="frete4">R$ {{frete4}},00</td>
      <td class="frete5">R$ {{frete5}},00</td>
      <td class="frete6">R$ {{frete6}},00</td>
      <td class="frete7">R$ {{frete7}},00</td>
      <td class="frete8">R$ {{frete8}},00</td>

      {{!-- ------------------ --}}

      <td class="valor_final1">R$ {{valor_final1}},00</td>
      <td class="valor_final2">R$ {{valor_final2}},00</td>
      <td class="valor_final3">R$ {{valor_final3}},00</td>
      <td class="valor_final4">R$ {{valor_final4}},00</td>
      <td class="valor_final5">R$ {{valor_final5}},00</td>
      <td class="valor_final6">R$ {{valor_final6}},00</td>
      <td class="valor_final7">R$ {{valor_final7}},00</td>
      <td class="valor_final8">R$ {{valor_final8}},00</td>

      {{!-- ------------------ --}}

      <td>R$ {{valor_nota}},00</td>
    </tr>
  </tbody>
  {{/data}}
</table>

